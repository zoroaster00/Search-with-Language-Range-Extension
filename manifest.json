{
  "name": "Google\u2122 Search with Language Range",
  "version": "0.1.1",
  "description": "Search in Google with lanaguage you choose.",
  "default_locale": "en",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://www.google.com/search?*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "manifest_version": 2
}