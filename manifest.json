{
  "name": "Google\u2122 Search with Language Range",
  "version": "0.1.0",
  "description": "Search in Google with lanaguage you choose.",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Google Search with Language Range",
      "keyword": "Google Search with Language Range",
      "search_url": "http://www.google.com/search?lr=lang_zh-TW&q={searchTerms}&glr_id",
      "favicon_url": "http://www.google.com/favicon.ico",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "manifest_version": 2
}